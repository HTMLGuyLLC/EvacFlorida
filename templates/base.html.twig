<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}EvacFlorida.com{% endblock %}</title>
        {{ encore_entry_link_tags('app') }}
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% block header %}
            <header>
                <div class="container">
                    <div class="row">
                        <div class="col-md-6">
                            <h1>EvacFlorida.com</h1>
                        </div>
                        <div class="col-md-6">
                            <!-- menu goes here -->
                        </div>
                    </div>
                </div>
            </header>
        {% endblock %}
        <div class="container main-content">
            {% block body %}{% endblock %}
        </div>
        {% block footer %}
            <footer>
                <div class="container">
                    <div class="row">
                        <div class="col-sm-12">
                            &copy; {{ "now"|date("Y") }} EvacFlorida.com
                        </div>
                    </div>
                </div>
            </footer>
        {% endblock %}
        {{ encore_entry_script_tags('app') }}
        {% block javascripts %}{% endblock %}
    </body>
</html>
